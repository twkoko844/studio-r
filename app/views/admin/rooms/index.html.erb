<div class="lbody">
<% @page_title = "部屋一覧" %>
<h2><%= @page_title %></h2>
<div class="box">
<% if @rooms.present? %>
<table class="list">
  <tr>
    <th>部屋id</th>
    <th>部屋名</th>
  </tr>
<% @rooms.each do |room| %>
  <tr>
    <td><%= room.id %></td>
    <td><%= link_to room.name , [:admin, room] %></td>
    <td><%= room_image_tag room %></td>
  </tr>
<% end %>
</table>
</br>
<% end %>
</div>
</div>
